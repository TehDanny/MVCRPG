@model List<MVCRPG.Models.Character>

@{
    ViewBag.Title = "Index";
}

@using (Html.BeginForm())
{
    <h2>Characters:</h2>
}

<div>
    
        <ul>
            @foreach (var item in Model)
            {
            <li>
                <label>@item.Name</label>
            </li>
            }
        </ul>
</div>

@Html.ActionLink("Create character", "Create", "Character")<br />
@Html.ActionLink("Return to home", "Index", "Home")

